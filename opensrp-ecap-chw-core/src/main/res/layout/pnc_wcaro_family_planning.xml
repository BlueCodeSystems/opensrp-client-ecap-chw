<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <org.smartregister.view.customcontrols.CustomFontTextView
        android:id="@+id/pncFamilyPlanningMethod"
        style="@style/CustomFontTextViewStyle.ClientList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:text="@string/pnc_family_planning_method"
        android:textColor="@color/medical_text_inner"
        android:textSize="16sp"
        android:visibility="gone"
        />

    <org.smartregister.view.customcontrols.CustomFontTextView
        android:id="@+id/pncFamilyPlanningDate"
        style="@style/CustomFontTextViewStyle.ClientList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:text="@string/pnc_family_planning_date"
        android:textColor="@color/medical_text_inner"
        android:textSize="16sp"
        android:visibility="gone"
        />


</LinearLayout>
